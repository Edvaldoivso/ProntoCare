<section id="Graficos">
    <div id="curve_chart" style="width: 100%; height: 300px">Histórico de Saúde

<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawChart);

      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Temperatura', 'Pressão', 'Hemoglobina'],
          ['2004',  1000,      400],
          ['2005',  1170,      460],
          ['2006',  660,       1120],
          ['2007',  1030,     1600]
        ]);

        var options = {
          title: 'Balanço Sanguineo',
          curveType: 'function',
          legend: { position: 'bottom' }
        };

        var chart = new google.visualization.LineChart(document.getElementById('curve_chart'));

        chart.draw(data, options);
      }
    </script>
 





            </div>
</br>

            <div  id="Peso" style="width: 100%; height: 300px">


    <script type="text/javascript">
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawChart);

      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Massa Corporea', 'Massa Magra', 'Massa Gorda'],
          ['2004',  1000,      40],
          ['2005',  1170,      460],
          ['2006',  400,       6020],
          ['2007',  2030,     1000]
        ]);

        var options = {
          title: 'Controle de Peso',
          curveType: 'function',
          legend: { position: 'bottom' }
        };

        var chart = new google.visualization.LineChart(document.getElementById('Peso'));

        chart.draw(data, options);
      }
    </script>
                
            </div>

            </section>


